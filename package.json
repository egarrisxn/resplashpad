    "clsx": "^2.1.1",
    "lucide-react": "^0.483.0",
    "react": "^18.3.1",
    "react-blurhash": "^0.3.0",
    "react-dom": "^18.3.1",
    "react-router": "^7.4.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@tanstack/react-query-devtools": "^5.61.5",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
